<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>服务端渲染</title>
    <script src="https://unpkg.com/vue@3/dist/vue.global.js"></script>
</head>

<body>
    <button id="btn">
        <%= msg %>
    </button>

    <div id="app">
        <button @click="count++">
            Count is: {{ count }}
        </button>
    </div>
    <script>
        const { createApp } = Vue
        createApp({
            data() {
                return {
                    count: 0
                }
            }
        }).mount('#app')
    </script>
    <script>
        const el = document.getElementById('btn')
        console.log(el)
        el.addEventListener('click', () => {
            alert('我被点击了')
        })
    </script>
</body>

</html>